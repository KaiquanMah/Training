# Import numpy
import numpy as np

# Assign the filename: file
file = 'digits_header.txt'

# Load the data: data
# comma separated ',', tab delimited '\t'
# skiprows = # of rows to skip
# usecols = [index of the cols to keep]
data = np.loadtxt(file, delimiter='\t', skiprows=1, usecols=[0,2])

# Print data
print(data)
