# Import packages
# seaborn statistical data viz package: http://seaborn.pydata.org/
import matplotlib.pyplot as plt
import seaborn as sns

# Set seaborn style
sns.set(color_codes=True)

# Create a list of labels:cd
cd = ['clinton', 'trump', 'sanders', 'cruz']

# Create a list of variables: v
v = [clinton, trump, sanders, cruz]

# Plot histogram by passing in a list of labels and a list containing the variables to plot
ax = sns.barplot(cd, v)
ax.set(ylabel="count")
plt.show()
