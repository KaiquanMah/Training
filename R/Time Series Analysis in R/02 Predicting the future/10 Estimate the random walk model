# Plot rw_diff
ts.plot(rw_diff)
data:image/png;base64,

>mean(rw_diff)
[1] 0.5811833









# Now fit the WN model to the differenced data
model_wn <- arima(rw_diff, order = c(0,0,0))
model_wn

Call:
arima(x = rw_diff, order = c(0, 0, 0))

Coefficients:
      intercept
         0.5812
s.e.     0.1128

sigma^2 estimated as 1.273:  log likelihood = -153.97,  aic = 311.93


# Store the value of the estimated time trend (intercept)
int_wn <- model_wn$coef
>int_wn
intercept 
0.5811833 




# Plot the original random_walk data
ts.plot(random_walk)

