# house list of lists
house = [["hallway", 11.25], 
         ["kitchen", 18.0], 
         ["living room", 20.0], 
         ["bedroom", 10.75], 
         ["bathroom", 9.50]]
         
# initialise index
index = 0

# Build a for loop from scratch
for room, area in house:
    room = house[index][0]
    area = house[index][1]
    print("the ", room, "is ", area, "sqm")
    index += 1
